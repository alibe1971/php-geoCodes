{
    "name": "alibe/php-geocodes",
    "description": "Script for retrieving and searching information about the ISO-3166 country code",
    "type": "library",
    "keywords": [
        "country code",
        "ISO-3166"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Liberati",
            "homepage": "https://alibe.ie"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/string": "^6.3",
        "symfony/translation-contracts": "^3.3",
        "symfony/intl": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Alibe\\GeoCodes\\": "src/"
        },
        "exclude": [
            "dev",
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alibe\\GeoCodes\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.51",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
